{
    "home": {
        "type": "UserActionScreen",
        "header": "Chip Diagnostics",
        "description": "Ready for a new test with Small TENPO protocol 23v3. Press 'Start' to begin.",
        "next_text": "Start"
    },

    "summary":{
        "type": "UserActionScreen",
        "next_text": "Start experiment"
    },
    
    "reset_start": {
        "type": "MachineActionScreen",
        "header": "Initialization",
        "description": "Initializing device. Resetting syringe positions and checking connections.",
        "action": {
            "RESET": {}
        },
        "remove_progress_bar": true,
        "completion_msg": "Machine has been homed"
    },
    "insert_syringes": {
        "type": "UserActionScreen",
        "header": "Insert syringes",
        "description": "Insert the waste and lysate syringe. Press 'Start initialization' to initialize when ready.",
        "next_text": "Start initialization"
    },
    "grab_syringes": {
        "type": "MachineActionScreen",
        "header": "Grabbing syringes",
        "description": "The device is now grabbing hold of the syringes to secure a precise operation.",
        "action": {
            "GRAB": {
                "post_run_rate_mm": 5,
                "post_run_vol_ml": 0.3
            }
        },
        "remove_progress_bar": true,
        "completion_msg": "Syringes ready"
    },
    "insert_chip": {
        "type": "UserActionScreen",
        "header": "Insert Kit",
        "description": "Insert the chip. Press 'Next' to proceed."
    },
    "f127": {
        "type": "UserActionScreen",
        "header": "Add F-127",
        "description": "Add 1.0 mL 1% F-127 in 1xPBS to reservoir. Press 'Next' to start."
    },
    "flush_1": {
        "type": "MachineActionScreen",
        "header": "F-127 pull",
        "description": "Wetting the chip with F-127",
        "action": {
            "PUMP": {
                "target": "waste",
                "vol_ml": 0.5,
                "rate_mh": 15,
                "eq_time": 0
            }
        }
    },
    "incubate_1": {
        "type": "MachineActionScreen",
        "header": "Blocking",
        "description": "Blocking chip with F-127",
        "action": {
            "INCUBATE": {
                "time": 3600
            }
        },
        "completion_msg": "F-127 blocking finished."
    },

    "flush_2": {
        "type": "MachineActionScreen",
        "header": "F-127 pull",
        "description": "Wetting the chip with F-127",
        "action": {
            "PUMP": {
                "target": "waste",
                "vol_ml": 0.5,
                "rate_mh": 15,
                "eq_time": 0
            }
        },
        "completion_msg": "F-127 Pull Finished."
    },

    "pbs_1": {
        "type": "UserActionScreen",
        "header": "PBS rinse",
        "description": "Add 1 mL 1xPBS to reservoir. Press 'Next' to start."
    },

    "flush_3": {
        "type": "MachineActionScreen",
        "header": "PBS rinse",
        "description": "Rinsing the chip.",
        "action": {
            "PUMP": {
                "target": "waste",
                "vol_ml": 1.0,
                "rate_mh": 15,
                "eq_time": 0
            }
        },
        "completion_msg": "PBS rinse complete"
    },
    "add_sample": {
        "type": "UserActionScreen",
        "header": "Add sample",
        "description": "Add 0.5 mL sample to reservoir. Press 'Next' to start"
    },
    "flush_4": {
        "type": "MachineActionScreen",
        "header": "Sample pull",
        "description": "Pulling sample thru chip.",
        "action": {
            "PUMP": {
                "target": "waste",
                "vol_ml": 0.50,
                "rate_mh": 1.0,
                "eq_time": 0
            }
        },
        "completion_msg": "Sample pull completed"
    },
    "pbs_2": {
        "type": "UserActionScreen",
        "header": "PBS Wash 1",
        "description": "Add 700 µL 1xPBS to reservoir. Press 'Next' to start."
    },
    "wash_1": {
        "type": "MachineActionScreen",
        "header": "PBS Wash 1",
        "description": "Washing the chip.",
        "action": {
            "PUMP": {
                "target": "waste",
                "vol_ml": 0.7,
                "rate_mh": 15,
                "eq_time": 0
            }
        },
        "completion_msg": "Wash 1 complete"
    },
    "pbs_3": {
        "type": "UserActionScreen",
        "header": "PBS Wash 2",
        "description": "Add 700 µL 1xPBS to reservoir. Press 'Next' to start."
    },
    "flush_5": {
        "type": "MachineActionScreen",
        "header": "PBS Wash 2",
        "description": "Washing the chip.",
        "action": {
            "PUMP": {
                "target": "waste",
                "vol_ml": 0.7,
                "rate_mh": 15,
                "eq_time": 0
            }
        },
        "completion_msg": "Wash 2 complete"
    },

    "pbs_4": {
        "type": "UserActionScreen",
        "header": "PBS Wash 3",
        "description": "Add 700 µL 1xPBS to reservoir. Press 'Next' to start."
    },
    "flush_6": {
        "type": "MachineActionScreen",
        "header": "PBS Wash 3",
        "description": "Washing the chip.",
        "action": {
            "PUMP": {
                "target": "waste",
                "vol_ml": 0.7,
                "rate_mh": 15,
                "eq_time": 0
            }
        },
        "completion_msg": "Wash 3 complete"
    },

    "protein_lysis": {
        "type": "UserActionScreen",
        "header": "Protein Lysis",
        "description": "Add 300 µL Protein Lysis to reservoir. Press 'Next' to start."
    },

    "extract_1": {
        "type": "MachineActionScreen",
        "header": "Protein Lysis",
        "description": "Pulling Protein Lysis into chip.",
        "action": {
            "PUMP": {
                "target": "lysate",
                "vol_ml": 0.05,
                "rate_mh": 15,
                "eq_time": 240
            }, 
            "RELEASE": {
                "target": "waste",
                "vol_ml" : 1.5,
                "rate_mh" : -50,
                "eq_time": 0
            }
        }
        
    },

    "chase_1": {
        "type": "MachineActionScreen",
        "header": "Protein Lysis Final Extraction",
        "description": "Extracting lysate from chip.",
        "action": {
            "PUMP": {
                "target": "lysate",
                "vol_ml": 0.75,
                "rate_mh": 15,
                "eq_time": 0
            }
        },
        "completion_msg": "Protein Lysis pull and incubation complete"
    },

    "remove_kit": {
        "type": "UserActionScreen",
        "header": "Remove kit",
        "description": "Remove used kit from machine."
    },
    "reset_end": {
        "type": "MachineActionScreen",
        "header": "Homing device",
        "description": "Resetting syringe pump positions.",
        "action": {
            "RESET": {}
        },
        "remove_progress_bar": true,
        "completion_msg": "App will restart so a new test may begin."
    }
}