{
    "home": {
        "type": "UserActionScreen",
        "header": "Chip Diagnostics",
        "description": "Ready for a new test with protocol 200v0. Press 'Start' to begin.",
        "next_text": "Start"
    },
    "reset_start": {
        "type": "MachineActionScreen",
        "header": "Initialization",
        "description": "Initializing device. Resetting syringe positions and checking connections.",
        "action": {
            "RESET": {}
        },
        "remove_progress_bar": true,
        "completion_msg": "Machine has been homed"
    },
    "insert_syringes": {
        "type": "UserActionScreen",
        "header": "Insert syringes",
        "description": "Insert the waste and lysate syringe. Press 'Start initialization' to initialize when ready.",
        "next_text": "Start initialization"
    },
    "grab_syringes": {
        "type": "MachineActionScreen",
        "header": "Grabbing syringes",
        "description": "The device is now grabbing hold of the syringes to secure a precise operation.",
        "action": {
            "GRAB": {
                "post_run_rate_mm": 5,
                "post_run_vol_ml": 0.3
            }
        },
        "remove_progress_bar": true,
        "completion_msg": "Syringes ready"
    },
    "insert_chip": {
        "type": "UserActionScreen",
        "header": "Insert Kit",
        "description": "Insert the chip and inlet reservoir. Press 'Next' to proceed."
    },
    "Wetting the chip with F-127_1": {
        "type": "UserActionScreen",
        "header": "Add F-127",
        "description": "Add 0.5 ml of F-127"
    },
    "Wetting the chip with F-127": {
        "type": "MachineActionScreen",
        "header": "Add F-127",
        "description": "Wetting the chip with F-127",
        "action": {
            "PUMP": {
                "target": "waste",
                "vol_ml": 0.5,
                "rate_mh": 15,
                "eq_time": 0
            }
        }
    },
    "Blocking chip with F-127": {
        "type": "MachineActionScreen",
        "header": "Add F-127",
        "description": "Blocking chip with F-127",
        "action": {
            "INCUBATE": {
                "time": 3600
            }
        }
    },
    "Rinsing the chip._1": {
        "type": "UserActionScreen",
        "header": "Add PBS",
        "description": "Add 1.0 ml of PBS"
    },
    "Rinsing the chip.": {
        "type": "MachineActionScreen",
        "header": "Add PBS",
        "description": "Rinsing the chip.",
        "action": {
            "PUMP": {
                "target": "waste",
                "vol_ml": 1.0,
                "rate_mh": 15,
                "eq_time": 0
            }
        }
    },
    "Pulling sample thru chip._1": {
        "type": "UserActionScreen",
        "header": "Add Sample",
        "description": "Add 0.5 ml of Sample"
    },
    "Pulling sample thru chip.": {
        "type": "MachineActionScreen",
        "header": "Add Sample",
        "description": "Pulling sample thru chip.",
        "action": {
            "PUMP": {
                "target": "waste",
                "vol_ml": 0.5,
                "rate_mh": 1.0,
                "eq_time": 0
            }
        }
    },
    "Washing the chip._1": {
        "type": "UserActionScreen",
        "header": "Add PBS",
        "description": "Add 0.7 ml of PBS"
    },
    "Washing the chip.": {
        "type": "MachineActionScreen",
        "header": "Add PBS",
        "description": "Washing the chip.",
        "action": {
            "PUMP": {
                "target": "waste",
                "vol_ml": 0.7,
                "rate_mh": 15.0,
                "eq_time": 0
            }
        }
    },
    "Pulling Qiazol into chip._1": {
        "type": "UserActionScreen",
        "header": "Add Qiazol",
        "description": "Add 0.2 ml of Qiazol"
    },
    "Pulling Qiazol into chip.": {
        "type": "MachineActionScreen",
        "header": "Add Qiazol",
        "description": "Pulling Qiazol into chip.",
        "action": {
            "PUMP": {
                "target": "lysate",
                "vol_ml": 0.2,
                "rate_mh": 15.0,
                "eq_time": 150
            },
            "RELEASE": {
                "target": "waste",
                "vol_ml": 1.5,
                "rate_mh": -50,
                "eq_time": 0
            }
        }
    },
    "Extracting lysate from chip._1": {
        "type": "UserActionScreen",
        "header": "Add Qiazol",
        "description": "Add 1.0 ml of Qiazol"
    },
    "Extracting lysate from chip.": {
        "type": "MachineActionScreen",
        "header": "Add Qiazol",
        "description": "Extracting lysate from chip.",
        "action": {
            "PUMP": {
                "target": "lysate",
                "vol_ml": 1.0,
                "rate_mh": 15.0,
                "eq_time": 0
            }
        }
    },
    "remove_kit": {
        "type": "UserActionScreen",
        "header": "Remove kit",
        "description": "Remove used kit from machine."
    },
    "reset_end": {
        "type": "MachineActionScreen",
        "header": "Homing device",
        "description": "Resetting syringe pump positions.",
        "action": {
            "RESET": {}
        },
        "remove_progress_bar": true,
        "completion_msg": "App will restart so a new test may begin."
    }
}